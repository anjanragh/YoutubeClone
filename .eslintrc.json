{
    "extends": [
        "plugin:flowtype/recommended",
        "airbnb",
        "prettier",
        "prettier/react"
    ],
    "plugins": [
        "flow-type",
        "prettier"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2016,
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "jest": true
    }
}